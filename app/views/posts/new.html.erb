<div class="container mt-4">
  <div class="row">
    <div class="col">
      <h4>投稿フォーム</h4>
      <%= form_with model: @post, local:true do |f| %>
        <h4>タイトル</h4>
        <%= f.text_field :title %>
        <h4>プレー日</h4>
        <%= f.date_select :date %>
        <h4>スコア</h4>
        <%= f.text_field :score %>
        <h4>画像</h4>
        <%= f.attachment_field :post_images_images, multiple: true %>
        <h4>感想</h4>
        <%= f.text_area :impression %>
        <%= f.submit '投稿' %>
      <% end %>
    </div>
  </div>
</div>