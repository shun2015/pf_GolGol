<%= search_form_for @q do |f| %>
  <%= f.label :average_score, "平均スコア" %>
  <%= f.select :average_score_eq, [["130代", "130"],["120代", "120"], ["110代", "110"], ["100代", "100"], ["90代", "90"], ["80代", "80"], ["70代", "70"], ["60代", "60"]], {include_blank: "選択して下さい"}, class: 'user' %><br />
  
  <%= f.label :gender, "性別" %>
　<%= f.radio_button :gender_eq, '', {:checked => true} %>指定なし
  <%= f.radio_button :gender_eq, 0 %>男性
  <%= f.radio_button :gender_eq, 1 %>女性<br />
  
  <%= f.submit "検索" %>
<% end %>

<% @users.each do |user| %>
  <%= link_to user_path(user) do %>
    <%= user.name%>
  <% end %>
  <%= user.average_score %>
  <%= user.age %>
  <%= user.gender %>
  <%= user.address %><br>
<% end %>